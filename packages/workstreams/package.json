{
  "name": "@agenv/workstreams",
  "version": "0.1.0",
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "type": "module",
  "bin": {
    "work": "./dist/bin/work.js"
  },
  "files": [
    "dist/",
    "README.md"
  ],
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js"
    },
    "./types": {
      "types": "./dist/lib/types.d.ts",
      "import": "./dist/lib/types.js"
    },
    "./cli/*": {
      "types": "./dist/cli/*.d.ts",
      "import": "./dist/cli/*.js"
    }
  },
  "scripts": {
    "build": "rm -rf dist && bun build.ts",
    "test": "bun test",
    "typecheck": "tsc --noEmit",
    "prepublishOnly": "bun run build"
  },
  "devDependencies": {
    "@types/bun": "^1.3.6",
    "@types/marked": "^6.0.0",
    "@types/proper-lockfile": "^4.1.4"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "marked": "^17.0.1",
    "proper-lockfile": "^4.1.2",
    "yaml": "^2.7.0"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/agenv/agenv.git",
    "directory": "packages/workstreams"
  },
  "license": "MIT",
  "keywords": [
    "workstream",
    "task-management",
    "cli",
    "workflow",
    "project-management",
    "agenv"
  ],
  "engines": {
    "node": ">=18.0.0"
  }
}